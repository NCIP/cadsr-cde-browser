      <pathelement location="${java.lib.dir}/commons-validator.jar"/>
      <pathelement location="${java.lib.dir}/jakarta-oro.jar"/> 
      
      <!-- Springframework libs -->
      <pathelement location="${java.lib.dir}/spring115.jar"/> 
      <pathelement location="${java.lib.dir}/spring-context115.jar"/> 
      <pathelement location="${java.lib.dir}/spring-core115.jar"/> 
      <pathelement location="${java.lib.dir}/spring-dao115.jar"/> 
      <pathelement location="${java.lib.dir}/spring-orm115.jar"/> 
      <pathelement location="${java.lib.dir}/spring-context115.jar"/>
      <pathelement location="${java.lib.dir}/spring-web115.jar"/>
            
      <!--J2EE  libs -->
      <pathelement location="${java.lib.dir}/servlet.jar"/>
      <pathelement location="${java.lib.dir}/jndi.jar"/>   
      <pathelement location="${java.lib.dir}/jta.jar"/>  
      <pathelement location="${java.lib.dir}/jdbc.jar"/>
