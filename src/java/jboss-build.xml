                <include name="gov/nih/nci/ncicb/cadsr/umlbrowser/service/**"/>
                <include name="gov/nih/nci/ncicb/cadsr/umlbrowser/util/**"/>
                <include name="gov/nih/nci/ncicb/cadsr/admin/**"/>
