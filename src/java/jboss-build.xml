          <include name="umlbrowser/**/*.*"/>  
          <include name="admin/**/*.*"/>  
          <include name="*.jsp"/>
        </patternset>
      </fileset>
      
      <fileset dir="${jsp.dir}/">
              <patternset>
                <include name="common/**/*.*"/>  
                <include name="*.jsp"/>
